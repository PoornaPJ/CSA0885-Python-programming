def count_odd_even(numbers):
    odd_count = 0
    even_count = 0

    for num in numbers:
        if num % 2 == 0:
            even_count += 1
        else:
            odd_count += 1

    return odd_count, even_count

numbers_list = [1, 2, 3, 4, 5, 6, ]
odd, even = count_odd_even(numbers_list)
print("Odd numbers:",odd)
print("Even numbers:",even)
