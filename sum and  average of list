def calculate_sum_average(numbers):
    sum_of_numbers = sum(numbers)
    average_of_numbers = sum_of_numbers / len(numbers)
    return sum_of_numbers, average_of_numbers

numbers_list = [1, 2, 3, 4, 5]
sum_of_nums, average_of_nums = calculate_sum_average(numbers_list)
print("Sum:", sum_of_nums)
print("Average:", average_of_nums)
